<%= form_tag students_path do %>
  <%= label_tag(:full_name, "Full name") %>
  <%= text_field_tag(:full_name) %>

  <%= label_tag(:iq, "IQ") %>
  <%= text_field_tag(:iq) %>

  <%= label_tag(:gpa, "GPA") %>
  <%= text_field_tag(:gpa) %>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag("Create student") %>
<% end %>
